<% provide(:title, $tools[:map][:title]) %>
<% provide(:version, $tools[:map][:version]) %>
<% provide(:icon, $tools[:map][:icon]) %>

<div class="tool-title">
    <i class="white <%= yield(:icon) %>"></i>
    <%= yield(:title) %>
</div>

<div class="container">
    <div class="loader mx-auto mt-5 invisible" id="printing"></div>

    <div id="mapid" style="margin-top: -160px; height: 800px; width: 100%;"></div>
</div>

<div class="version"><%= yield(:title) %> version <%= yield(:version) %></div>
